{
    "license": "proprietary",
    "description": "",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": [
                "src"
            ]
        }
    },
    "require": {
        "php": "^7.4",
        "ext-sockets": "*",
        "filp/whoops": "^2.7"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "scripts": {
        "cs_check": "phpcs ./src --standard=PSR12",
        "cs_fix": "phpcbf ./src --standard=PSR12"
    }
}
